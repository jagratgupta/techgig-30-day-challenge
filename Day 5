// Day 5 : Loop your world

import java.io.*;
import java.util.*;
import java.util.Scanner;
public class CandidateCode {
    public static void main(String args[] ) throws Exception {

    	//Write code here
        Scanner sc =new Scanner(System.in);
       int a=sc.nextInt();
        int fact=1;

    for(int i=1;i<=a;i++)
        {
            fact=fact*i;
        }
    System.out.print(fact+"");
   }
}