// Day 16 : GCD of two numbers


import java.io.*;
import java.util.*;
import java.util.Scanner;
public class CandidateCode {
    public static void main(String args[] ) throws Exception {

    	Scanner sc = new Scanner(System.in);
		 int n1 = sc.nextInt();  
           int n2 = sc.nextInt(); 
          while (n1 != n2) {
        	if(n1 > n2)
                n1 = n1 - n2;
            else
                n2 = n2 - n1;
        }
        System.out.print(n2);

    }        
}
