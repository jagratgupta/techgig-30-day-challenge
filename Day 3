// Day 3 : Play with operators

import java.io.*;
import java.util.*;
import java.util.Scanner;
public class CandidateCode {
    public static void main(String args[] ) throws Exception {

    	//Write code here   
        Scanner sc = new Scanner(System.in);
        int a= sc.nextInt();
        int b= sc.nextInt();
        int c= sc.nextInt();

        int d = (a*b*c)/100;

        System.out.print(d+"");
   }
}